{
  "name": "storytime-calendar-backend",
  "version": "1.0.0",
  "description": "AI-powered calendar storytelling backend service that transforms mundane calendar events into engaging, personalized narratives with multi-provider AI integration, Slack notifications, and comprehensive event management.",
  "main": "dist/server.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --project tsconfig.json src/server.ts",
    "build": "tsc",
    "start": "node -r tsconfig-paths/register dist/server.js",
    "start:prod": "NODE_ENV=production node -r tsconfig-paths/register dist/server.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "prisma:generate": "prisma generate --schema src/database/schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema src/database/schema.prisma",
    "prisma:migrate:prod": "prisma migrate deploy --schema src/database/schema.prisma",
    "prisma:studio": "prisma studio --schema src/database/schema.prisma",
    "typecheck": "tsc --noEmit",
    "test": "echo \"Tests not implemented yet\" && exit 0",
    "quality": "npm run prisma:generate && npm run typecheck && npm run lint:fix && npm run format && npm run build && echo '✅ Quality check completed successfully!'",
    "quality:check": "npm run prisma:generate && npm run typecheck && npm run lint && npm run build && echo '✅ Quality check completed successfully!'",
    "docker:build": "docker build -t storytime-calendar-backend .",
    "docker:run": "docker run -p 5004:5004 storytime-calendar-backend"
  },
  "keywords": [
    "ai",
    "calendar",
    "storytelling",
    "nodejs",
    "typescript",
    "prisma",
    "express",
    "slack",
    "notifications",
    "openai",
    "gemini",
    "anthropic",
    "google-calendar",
    "oauth",
    "rest-api",
    "backend",
    "microservice"
  ],
  "author": {
    "name": "Nakul Jhunjhunwala",
    "email": "nakul@example.com",
    "url": "https://github.com/nakuljhunjhunwala"
  },
  "contributors": [
    {
      "name": "Nakul Jhunjhunwala",
      "email": "nakul@example.com",
      "role": "Lead Developer"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nakuljhunjhunwala/storytime-calendar-backend.git"
  },
  "bugs": {
    "url": "https://github.com/nakuljhunjhunwala/storytime-calendar-backend/issues"
  },
  "homepage": "https://github.com/nakuljhunjhunwala/storytime-calendar-backend#readme",
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "@types/cron": "^2.0.1",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "cron": "^4.3.3",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-http-context": "^2.0.1",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^4.2.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-datadog-transport": "^1.0.2",
    "zod": "^4.0.2"
